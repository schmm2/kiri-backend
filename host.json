{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "fileLoggingMode": "always",
        "logLevel": {
            "default": "Warning",
            "Host.Results": "Warning",
            "Function": "Warning",
            "Host.Aggregator": "Warning"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[1.*, 2.0.0)"
    }
}